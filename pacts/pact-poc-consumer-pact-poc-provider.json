{
  "consumer": {
    "name": "pact-poc-consumer"
  },
  "interactions": [
    {
      "description": "a request to get discount #1",
      "pending": false,
      "providerStates": [
        {
          "name": "discount #1 exists"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/discounts/1"
      },
      "response": {
        "body": {
          "content": {
            "description": "5.8% off for Singaporean 58th national day",
            "id": 1,
            "title": "5.8% off",
            "type": "percentage",
            "value": 5.8
          },
          "contentType": "application/json",
          "encoded": false
        },
        "headers": {
          "Content-Type": [
            "application/json"
          ]
        },
        "matchingRules": {
          "body": {
            "$.description": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                }
              ]
            },
            "$.title": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.type": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.value": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "decimal"
                }
              ]
            }
          }
        },
        "status": 200
      },
      "transport": "http",
      "type": "Synchronous/HTTP"
    }
  ],
  "metadata": {
    "pactRust": {
      "ffi": "0.4.5",
      "mockserver": "1.1.1",
      "models": "1.1.2"
    },
    "pactSpecification": {
      "version": "4.0"
    }
  },
  "provider": {
    "name": "pact-poc-provider"
  }
}